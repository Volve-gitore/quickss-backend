{
  "name": "quickss-backend",
  "version": "1.0.0",
  "description": "QuiKss(quick-self-Serve) is an all in one Resto Menu and ordering mobile phone application that allows Restaurant and Hotels' customers to make orders (in &amp; out resto) using their portable smart phone and facilitate respective Restaurant and Hotels to quickly serve customers and keep data for future use",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --exec babel-node src/index",
    "test-seeds": "yarn sequelize-cli db:seed:undo:all && yarn sequelize-cli db:seed:all",
    "test": "yarn test-seeds && nyc --reporter=text mocha --require @babel/register --require @babel/polyfill ./tests/*.js --timeout 80000 --exit",
    "coverage": "npm test && nyc report --reporter=text-lcov | coveralls",
    "create-tables": "sequelize-cli db:migrate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Volve-gitore/quickss-backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Volve-gitore/quickss-backend/issues"
  },
  "homepage": "https://github.com/Volve-gitore/quickss-backend#readme",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "joi": "^17.2.1",
    "json-web-token": "^3.2.0",
    "moment": "^2.27.0",
    "nyc": "^15.1.0",
    "pg": "^8.3.3",
    "pg-hstore": "^2.3.3",
    "request": "^2.88.2",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/node": "^7.10.5",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.10.5",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^8.1.3",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.1",
    "travis": "^0.1.1"
  }
}
